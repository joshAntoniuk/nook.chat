{
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "airbnb/hooks",
    "next/core-web-vitals",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
    "rules": {
    "react/require-default-props": "off",
    "react/jsx-no-useless-fragment": "off",
    "react/no-array-index-key": "off"
  }
}
